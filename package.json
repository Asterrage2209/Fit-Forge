{
  "name": "fitforge-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently -k -n \"CLIENT,SERVER\" -c \"cyan,magenta\" \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "npm --prefix client run dev",
    "dev:server": "npm --prefix server run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
